 }\r\n});\r\n\r\n$(\".imageOverlay\").click(() => {\r\n  if (!playing) {\r\n    togglePause()\r\n  }\r\n})\r\n\r\n$(\"#songSubmit\").click(() => {\r\n  let params = new URLSearchParams();\r\n  params.append('name', $(\"#name\")[0].value);\r\n  params.append('song', $(\"#song\")[0].value);\r\n\r\n  axios({\r\n    url: backendURL + \"songRequest\", \r\n    data: params,\r\n    method: 'post'\r\n  })\r\n    .then(({data}) => {\r\n      if (data.includes(\"success\")) {\r\n        $(\"#songSubmit\").text(\"Submited\")\r\n        $(\"#songSubmit\").attr(\"disabled\", \"disabled\")\r\n        \r\n        setTimeout(() => {$(\"#songSubmit\").text(\"Submit\"); \r\n        $(\"#songSubmit\").attr(\"disabled\", false)}, 2000)\r\n      } else {\r\n      }\r\n    })\r\n})\r\n\r\n$(\".toggleplay\").click(() => {\r\n  togglePause()\r\n})\r\n\r\nvar overlayTimeout\r\n\r\nvar togglePause = () => {\r\n  if (!playing) {\r\n    audio.volume = volume;\r\n    audio.play()\r\n    $(\".playbutton\").hide()\r\n    $(\".pausebutton\").show()\r\n    playing = true\r\n  } else {\r\n    playing = false\r\n    audio.pause()\r\n    $(\".pausebutton\").hide()\r\n    $(\".playbutton\").show()\r\n  }\r\n}\r\n\r\n$(\".slider\").on(\"input\", (data) => {\r\n  volume = data.target.value / 100;\r\n  audio.volume = volume;\r\n})\r\n\r\nMicroModal.init();"},"lineCount":null}},"error":null,"hash":"0da823ef05037fe0404ce69d7651a0c9","cacheData":{"env":{}}}