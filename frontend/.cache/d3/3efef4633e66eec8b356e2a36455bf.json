r overlayTimeout\r\n\r\nvar togglePause = () => {\r\n  if (!playing) {\r\n    $(\"#audio\")[0].volume = 0\r\n    $(\"#audio\")[0].play()\r\n    $(\"#audio\").animate({volume: 0.5}, 1000);\r\n    $(\".playbutton\").hide()\r\n    $(\".imageOverlay\").css(\"opacity\", \"0\")\r\n    clearTimeout(overlayTimeout)\r\n    overlayTimeout = setTimeout(() => {\r\n      $(\".imageOverlay\").hide()\r\n    }, 300)\r\n    $(\".pausebutton\").show()\r\n    playing = true\r\n    console.log(\"E\")\r\n  } else {\r\n    playing = false\r\n    $(\"#audio\")[0].pause()\r\n    $(\".pausebutton\").hide()\r\n    $(\".imageOverlay\").show()\r\n    $(\".imageOverlay\").css(\"opacity\", \"1\")\r\n    $(\".playbutton\").show()\r\n  }\r\n}\r\n\r\n$(\".slider\").on(\"input\", (data) => {\r\n  $(\"#audio\")[0].volume = data.target.value / 100\r\n})\r\n\r\nMicroModal.init();"},"lineCount":null}},"error":null,"hash":"e02997f3204eebdadf6a24ad767ab17c","cacheData":{"env":{}}}